{
  "name": "tester",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "pack:pkg": "cd ../react-native-harmony-screens && npm pack && cd ../tester",
    "install:pkg": "npm uninstall react-native-harmony-web-view && npm run pack:pkg && npm i react-native-harmony-screens@file:../react-native-harmony-screens/rnoh-react-native-harmony-screens-3.25.0.tgz",
    "dev": "npm run install:pkg && react-native bundle-harmony",
    "android": "react-native run-android",
    "postinstall": "react-native unpack-harmony"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-harmony": "npm:@rnoh/react-native-harmony@0.72.3",
    "react-native-harmony-screens": "file:../react-native-harmony-screens/rnoh-react-native-harmony-screens-3.25.0.tgz"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@rnoh/testerino": "^0.0.1",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.2.15",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-config": "^0.76.3",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
